  <h:head><!-- ODK Aggregate upload time: 2017-10-19T06:58:38.705+0000 on https://csisaodk.appspot.com -->
    <h:title>CSISA ZT Wheat Service Provider</h:title>
    <model>
      <instance>
        <ZT_Wheat_SP_Bihar_EUP id="CSISA_ZT_Wheat_SP">
          <start/>
          <end/>
          <today/>
          <deviceid/>
          <phonenumber/>
          <acknowledge_test/>
          <EnumID/>
          <Hub/>
          <District/>
          <Block/>
          <GramPanchayat/>
          <Village/>
          <Name/>
          <Mobile/>
          <ServiceStartYear/>
          <OtherMachines/>
          <GPS/>
          <meta>
            <instanceID/>
          </meta>
        </ZT_Wheat_SP_Bihar_EUP>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/ZT_Wheat_SP_Bihar_EUP/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/ZT_Wheat_SP_Bihar_EUP/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/ZT_Wheat_SP_Bihar_EUP/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/ZT_Wheat_SP_Bihar_EUP/deviceid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/ZT_Wheat_SP_Bihar_EUP/phonenumber" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/acknowledge_test" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/EnumID" required="true()" type="select1"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/Hub" required="true()" type="select1"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/District" required="true()" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/Block" required="true()" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/GramPanchayat" required="true()" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/Village" required="true()" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/Name" required="true()" type="string"/>
      <bind constraint="string-length(.)=10" jr:constraintMsg="Make sure it has 10 digits" nodeset="/ZT_Wheat_SP_Bihar_EUP/Mobile" required="true()" type="string"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/ServiceStartYear" required="true()" type="select1"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/OtherMachines" required="true()" type="select"/>
      <bind nodeset="/ZT_Wheat_SP_Bihar_EUP/GPS" required="true()" type="geopoint"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ZT_Wheat_SP_Bihar_EUP/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
